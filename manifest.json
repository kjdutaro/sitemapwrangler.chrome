{
  "manifest_version": 3,
  "name": "__MSG_extension_name__",
  "description": "__MSG_short_description__",
  "default_locale": "en",
  "version": "0.9",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "pop.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "welcome.html",
        "welcome.css",
        "welcome.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
